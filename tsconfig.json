{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "backend/**/*",
    "node_modules/**/*",
    "android/**/*",
    "ios/**/*",
    "backup/**/*"
  ]
}
