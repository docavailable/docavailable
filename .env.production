# Production Environment Variables for DocAvailable
# This file contains production-specific environment variables

# Laravel API Configuration
EXPO_PUBLIC_API_BASE_URL=https://docavailable-3vbdv.ondigitalocean.app
EXPO_PUBLIC_LARAVEL_API_URL=https://docavailable-3vbdv.ondigitalocean.app

# App Configuration
EXPO_PUBLIC_APP_NAME=DocAvailable
EXPO_PUBLIC_APP_VERSION=1.0.0

# Feature Flags (for production)
EXPO_PUBLIC_USE_BACKEND_CHAT=true
EXPO_PUBLIC_USE_BACKEND_WALLET=true
EXPO_PUBLIC_USE_BACKEND_NOTIFICATIONS=true
EXPO_PUBLIC_USE_BACKEND_APPOINTMENTS=true
EXPO_PUBLIC_BACKEND_ENABLED=true

# Google OAuth Configuration (set these in EAS secrets)
EXPO_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id_here
EXPO_PUBLIC_GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Payment Gateway Configuration (set these in EAS secrets)
PAYCHANGU_API_KEY=your_paychangu_api_key_here
PAYCHANGU_SECRET_KEY=your_paychangu_secret_key_here
PAYCHANGU_MERCHANT_ID=your_paychangu_merchant_id_here
PAYCHANGU_WEBHOOK_SECRET=your_paychangu_webhook_secret_here
PAYCHANGU_ENVIRONMENT=production
EXPO_PUBLIC_PAYCHANGU_PUBLIC_KEY=your_paychangu_public_key_here

# AI Service API Keys (set these in EAS secrets)
DEEPSEEK_API_KEY=your_deepseek_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Other API keys (set these in EAS secrets)
PAYMENT_API_KEY=your_payment_api_key_here
SMS_API_KEY=your_sms_api_key_here
EMAIL_API_KEY=your_email_api_key_here
